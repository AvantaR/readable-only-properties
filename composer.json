{
    "name": "avantar/readable-only-properties",
    "description": "A little trait which blocks class properties setters.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Szala",
            "email": "avantar@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-with-clover": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-clover=coverage.xml"
    },
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ReadableOnlyProperties\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/unit"
        }
    }
}
